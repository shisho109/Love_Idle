<div class="photo_page">
<div class="title"><h1>(画像投稿ページ)</h1></div>
<%= form_for @photo do |f| %>

    <div class="field">
    <%= f.label :グループ名 %>
    <%= f.text_field :group, :size => 30 %>
  </div>

  <div class="field">
    <%= f.label :名前 %>
    <%= f.text_field :name, :size => 30 %>
  </div>

  <div class="field">
    <%= f.label :説明  %>
    <%= f.text_area :about, :size => "100x5" %>
  </div>

  <div class="field">
    <%= f.label :画像を選択 %>
    <%= f.file_field :image %>
  </div>

  <div class="submit">
  <%="推しの画像を投稿する！"%>
  </div>
<% end %>

<div class="toppage">
<%= link_to "画像投稿一覧に戻る", photos_path %>
</div>
</div>